{
  "name": "sakuramaru",
  "version": "0.6.3",
  "description": "3D Aerial Photograph Viewer",
  "main": "main.js",
  "scripts": {
    "clean": "shx rm -rf dist release index.html",
    "build": "webpack-cli && shx cp dist/index.html .",
    "server": "http-server -c-1",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.json mocha --timeout 30000 --exit -r ts-node/register test/**/*.ts",
    "test:file": "cross-env TS_NODE_PROJECT=tsconfig.json mocha --timeout 300000 --exit -r ts-node/register",
    "lint": "tslint --fix --project ."
  },
  "keywords": [
    "TypeScript",
    "React"
  ],
  "author": "Ryoji Kato <ryokat3@gmail.com>",
  "license": "ISC",
  "build": {
    "appId": "net.canadian-wisteria.sakuramaru",
    "directories": {
      "app": "./dist",
      "output": "./release"
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "boost-ts": "^2.2.0",
    "commander": "^8.3.0",
    "flux-standard-action": "^2.1.2",
    "fp-ts": "^2.11.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "chai": "^4.3.4",
    "copy-pkg": "^1.0.0",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.0.0",
    "inline-source-webpack-plugin": "^2.0.1",
    "mocha": "^9.1.3",
    "shx": "^0.3.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
