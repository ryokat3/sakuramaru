{
  "name": "sakuramaru",
  "version": "0.5.1",
  "description": "3D Aerial Photograph Viewer",
  "main": "main.js",
  "scripts": {
    "clean": "shx rm -rf dist release",
    "build": "webpack-cli && shx cp dist/index.html .",
    "release": "electron-builder build --win",
    "start": "electron ./dist",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.json mocha --timeout 30000 --exit -r ts-node/register test/**/*.ts",
    "test:file": "cross-env TS_NODE_PROJECT=tsconfig.json mocha --timeout 300000 --exit -r ts-node/register",
    "lint": "tslint --fix --project ."
  },
  "keywords": [
    "TypeScript",
    "Electron",
    "React"
  ],
  "author": "wak109@gmail.com",
  "license": "ISC",
  "build": {
    "appId": "net.canadian-wisteria.sakuramaru",
    "directories": {
      "app": "./dist",
      "output": "./release"
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "boost-ts": "^2.0.1",
    "commander": "^4.1.1",
    "electron": "^8.0.1",
    "flux-standard-action": "^2.1.1",
    "fp-ts": "^2.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/webpack": "^4.41.6",
    "chai": "^4.2.0",
    "copy-pkg": "^1.0.0",
    "cross-env": "^7.0.0",
    "electron-builder": "^21.2.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^7.0.1",
    "shx": "^0.3.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
