{
  "name": "sakuramaru",
  "version": "0.0.1",
  "description": "3D Aerial Photograph Viewer",
  "main": "main.js",
  "scripts": {
    "start": "electron ./dist",
    "build": "webpack-cli",
    "clean": "shx rm -rf dist release",
    "release": "electron-builder build --win",
    "lint": "tslint --project ."    
  },
  "keywords": [
    "TypeScript",
    "Electron",
    "React"
  ],
  "author": "wak109@gmail.com",
  "license": "ISC",
  "build": {
    "appId": "net.canadian-wisteria.sakuramaru",
    "directories": {
        "app": "./dist",
        "output": "./release"
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/webpack": "^4.41.0",
    "copy-pkg": "^1.0.0",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "html-webpack-plugin": "^3.2.0",
    "shx": "^0.3.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
